<VBox alignment="CENTER" prefHeight="312.0" prefWidth="497.0" spacing="40.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ec.edu.espol.turnerocovid19g4.EliminarPuestoController">
