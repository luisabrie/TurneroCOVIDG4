      <Label text="ELIMINAR PUESTO">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <HBox alignment="TOP_CENTER" prefHeight="27.0" prefWidth="600.0">
         <children>
            <Label prefHeight="17.0" prefWidth="70.0" text="Puesto" />
            <JFXComboBox fx:id="comboPuesto" prefHeight="25.0" prefWidth="131.0" />
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="73.0" prefWidth="520.0" spacing="40.0">
         <children>
            <JFXButton fx:id="bttEliminarPuesto" onAction="#eliminarPuesto" text="Eliminar Puesto" />
            <JFXButton fx:id="bttEliminarMedico" onAction="#eliminarMedico" text="Eliminar Medico" />
            <JFXButton fx:id="bttCerrar" onAction="#cerrar" text="Cerrar" />
         </children>
         <padding>
            <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
