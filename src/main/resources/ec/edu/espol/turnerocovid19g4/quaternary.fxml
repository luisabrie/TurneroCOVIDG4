<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="40.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ec.edu.espol.turnerocovid19g4.QuaternaryController">
   <children>
      <Label text="ADMINISTRACION PUESTO">
         <font>
            <Font name="System Bold" size="16.0" />
         </font></Label>
      <JFXButton fx:id="bttCrear" onAction="#crearPuesto" text="Crear" />
      <JFXButton fx:id="bttEliminar" onAction="#eliminarPuesto" text="Eliminar" />
      <JFXButton fx:id="bttAsignar" onAction="#asignarPuesto" text="Asignar" />
      <JFXButton fx:id="bttCerrar" onAction="#cerrar" text="Cerrar" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
   </padding>
</VBox>
