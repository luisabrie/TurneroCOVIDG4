<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="40.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ec.edu.espol.turnerocovid19g4.QuaternaryController">
   <children>
      <Label text="ADMINISTRACION PUESTO">
         <font>
            <Font name="System Bold" size="16.0" />
         </font></Label>
      <Button fx:id="bttCrear" mnemonicParsing="false" onAction="#crearPuesto" prefHeight="25.0" prefWidth="63.0" text="Crear" />
      <Button fx:id="bttEliminar" mnemonicParsing="false" onAction="#eliminarPuesto" prefHeight="25.0" prefWidth="67.0" text="Eliminar" />
      <Button fx:id="bttCerrar" mnemonicParsing="false" onAction="#cerrar" prefHeight="25.0" prefWidth="69.0" text="Cerrar" />
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="40.0" left="40.0" right="40.0" top="40.0" />
   </padding>
</VBox>
